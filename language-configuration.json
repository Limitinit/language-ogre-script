{
    "comments": {
        "lineComment": "//",
        "blockComment": [ "/*", "*/" ]
    },
    "brackets": [
        ["{", "}"]
    ],
    "surroundingPairs": [
        ["\"", "\""],
    ],
    "autoClosingPairs": [
        ["{", "}"],
        { "open": "\"", "close": "\"", "notIn": ["string", "comment"] },
        { "open": "/**", "close": " */", "notIn": ["string"] }
    ],
}